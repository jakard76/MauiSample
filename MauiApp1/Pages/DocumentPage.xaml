<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:drawable="clr-namespace:MauiApp1.Graphics"
             x:Class="MauiApp1.Pages.DocumentPage"
             Title="">
    <ContentPage.Resources>
        <drawable:WorkbookDrawable x:Key="drawable"/>
    </ContentPage.Resources>
    <VerticalStackLayout>
        <Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <GraphicsView Drawable="{StaticResource drawable}" HeightRequest="300" WidthRequest="400">
            
        </GraphicsView>
    </VerticalStackLayout>
</ContentPage>